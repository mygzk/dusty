<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="titleText1" format="string" />
    <attr name="titleTextColor1" format="color" />
    <attr name="titleTextSize1" format="dimension" />

    <attr name="image" format="reference" />

    <attr name="imageScaleType" format="enum">
        <enum name="fillXY" value="0" />
        <enum name="center" value="1" />
    </attr>

    <declare-styleable name="customTitleView">
        <attr name="titleText1" />
        <attr name="titleTextColor1" />
        <attr name="titleTextSize1" />
        <attr name="image" />
        <attr name="imageScaleType" />
    </declare-styleable>

    <declare-styleable name="customCircle">
        <attr name="firstColor" format="color" />
        <attr name="sendColor" format="color"/>
        <attr name="circleWidth" format="dimension"/>
        <attr name="circleSpeed" format="integer"/>

    </declare-styleable>




</resources>